    "author": "Doron Brayer <doronbrayer@outlook.com>",
    "license": "MIT",
    "homepage": "https://github.com/DoronBrayer/shuffrand",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DoronBrayer/shuffrand.git"
    },
    "bugs": {
